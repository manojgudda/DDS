#include <iostream>
using namespace std;

struct node {
    int data ;
    node*left;
    node* right;

    node(int val){
        data = val;
        left = nullptr;
        right = nullptr;
    }
};

void preorder(node * root ){
    if(root){
        cout << root -> data << " ";
        preorder (root -> left);
        preorder (root -> right);
    }
};

void inorder(node * root ){
    if (root){
        inorder (root -> left);
        cout << root -> data << " ";
        inorder (root -> right);
    }
};

void postorder (node * root ){
    if (root){
        postorder (root -> left);
        postorder (root ->right);
        cout << root -> data << " ";
    }
}

int main(){
node *root = new node (15);
root -> left = new node (10);
root -> right = new node (9);
root -> left -> left = new node (6);
root -> left -> right = new node (5);
root -> right -> left = new node (3);
root -> right -> right = new node (2);

cout <<"preorder :";
preorder(root);
cout<<endl;

cout << "inorder :";
inorder(root);
cout<<endl;

cout << "postorder :";
postorder(root);
cout<<endl;

return 0;
}

// tree diagram 
//                                        15

//           10                                                         9

// 6                  5                                       3                    2
