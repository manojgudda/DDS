
declare \\
vector <int> arr = {10,6,9};

methods\\
push_back -> add elemets in the back of array;
pop_back -> remove elemets in the back of array;
insert (position,value) -> insert at any position;


print\\
for ( int val : arr){
    cout<<val;
}

  #include <iostream>
#include <vector>

using namespace std;

int main (){
  vector <int> arr = {10,6,2};
  
  
  arr.push_back(6);
  arr.push_back(16);
  arr.push_back(5);
  arr.push_back(3);
  arr.push_back(1);
  
  arr.pop_back();
  arr.pop_back();
  
  cout<<"size of the array = "<<arr.size()<<endl;
  
  String lenght = arr.size() - 1;
  
  cout << lenght ;
  
  for (int val : arr){
      cout<<val<<" "<<endl;
  }
}
