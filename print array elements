#include <stdio.h>

int main() {
    int numbers[5] = {10, 20, 30, 40, 50};

    // Accessing elements
    printf("Third element: %d\n", numbers[2]);

    // Updating elements
    numbers[1] = 25;

    // Iterating through the array
    for (int i = 0; i < 5; i++) {
        printf("numbers[%d] = %d\n", i, numbers[i]);
    }

    return 0;
}
