//with while loop *********
#include <stdio.h>

int main() {
    int n ;
printf("enter a number :");
scanf("%d\n",&n);
int i = 1 ;
while (i <= n){
    printf("%d\n",i);
    i++;
}
    return 0;
}

// with for loop *******************
// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int n ;
printf("enter a number :");
scanf("%d\n",&n);
for ( int i = 1;i <= n; i++ ){
    printf("%d\n", i );
}

    return 0;
}

//with do while looop****************************
// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int n ;
printf("enter a number :");
scanf("%d\n",&n);
int i = 1;
do {
    printf("%d\n",i);
    i++;
}
while(i<=n);

    return 0;
}
